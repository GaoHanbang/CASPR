<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE cables [
  <!ATTLIST solver_class id ID #REQUIRED>
]>
<simulator>
	<solver_class id="FKLeastSquares"/>
    <solver_class id="FKDifferential"/>
    <solver_class id="FKHybridLeastSquaresDifferential"/>
    <plot_functions>
		<plot_function type="plotAngularVelocity">
            <figure_quantity>1</figure_quantity>
        </plot_function>
        <plot_function type="plotAngularAcceleration">
            <figure_quantity>1</figure_quantity>
        </plot_function>
        <plot_function type="plotBodyCoG">
            <figure_quantity>1</figure_quantity>
        </plot_function>
        <plot_function type="plotBodyCoGVelocity">
            <figure_quantity>1</figure_quantity>
        </plot_function>
        <plot_function type="plotBodyCoGAcceleration">
            <figure_quantity>1</figure_quantity>
        </plot_function>
        <plot_function type="plotJointPose">
            <figure_quantity>1</figure_quantity>
        </plot_function>
        <plot_function type="plotJointVelocity">
            <figure_quantity>1</figure_quantity>
        </plot_function>
        <plot_function type="plotCableLengths">
            <figure_quantity>1</figure_quantity>
        </plot_function>
        <plot_function type="plotCableLengthsVelocity">
            <figure_quantity>1</figure_quantity>
        </plot_function>
	</plot_functions>
</simulator>